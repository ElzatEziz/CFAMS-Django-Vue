er = this.$router\r\n      name = '/'+name\r\n      // if (name==='home'){\r\n      //   router.push('/index')\r\n      // }\r\n      router.push(name)\r\n\t\t\t}\r\n      \r\n\t\t}\r\n\t};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  .index-aside{\r\n    height: 100%;\r\n  }\r\n</style>\r\n"]}]}